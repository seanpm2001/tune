{
  "name": "tune",
  "version": "0.0.1",

  "files": ["dist"],
  "main": "dist/tune.umd.js",
  "module": "dist/tune.es.js",
  "types": "dist/types/index.d.ts",

   "scripts": {
    "build": "vite build && npm run build:types",
    "build:types": "vue-tsc --project tsconfig.build-types.json --declaration --emitDeclarationOnly --outDir dist/types ",
    "typecheck": "vue-tsc --project tsconfig.build-types.json --noEmit"
  },
  "prettier": "@snapshot-labs/prettier-config",
  "eslintConfig": {
    "extends": [
      "@snapshot-labs/vue"
    ]
  },
  "devDependencies": {
    "@babel/types": "^7.21.3",
    "@types/node": "^18.15.10",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/tsconfig": "^0.1.3",
    "sass": "^1.60.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  }
}
